<h1>Log In Below</h1>
<%= form_tag signin_path, method: "post" do %>
    <table><tr>
    <td><label>Username : </label></td>
    <td><%= text_field_tag :user_name %></td></tr>
    <tr><td><label>Password : </label></td>
    <td><%= password_field_tag :password %></td></tr>
    <tr><td><%= submit_tag 'Sign In' %></td></tr></table>
<% end %>
